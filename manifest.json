{
    "manifest_version": 2,
    "name": "Bookmarkets Scanner",
    "version": "1.0",
    "description": "scan duplicate bookmarkets",
    "browser_action": {
        "default_title": "BS"
    },
    "background": {
        "persistent": false,
        "scripts": [
            "js/background.js"
        ]
    },
    "permissions": [
        "bookmarks"
    ],
    "chrome_url_overrides": {
        "bookmarks": "bookmarksmod.html"
    }
}